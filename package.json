{
  "name": "personal-website",
  "version": "0.1.0",
  "homepage": "https://cjoecker.github.io/personal-website/",
  "private": true,
  "scripts": {
    "start": "ESLINT_NO_DEV_ERRORS=true react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "check-types": "tsc --noemit -p tsconfig.json",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d build",
    "lint": "eslint --max-warnings=0",
    "lint:fix": "yarn lint --fix",
    "lint:all": "yarn lint .",
    "lint:fix:all": "yarn lint:fix .",
    "format:all": "yarn format .",
    "format": "prettier --write"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@mui/material": "^5.2.6",
    "@mui/styled-engine": "^5.1.0",
    "@mui/styled-engine-sc": "^5.1.0",
    "@types/string-hash": "^1.1.1",
    "babel-loader": "8.2.3",
    "d3": "^7.0.4",
    "d3-ease": "^3.0.1",
    "date-fns": "^2.28.0",
    "framer-motion": "^5.5.5",
    "matter-js": "^0.18.0",
    "query-string": "^7.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-map-gl": "^5.3.17",
    "react-query": "^3.24.4",
    "react-scripts": "5.0.0",
    "react-use": "^17.3.2",
    "string-hash": "^1.1.3",
    "styled-components": "^5.3.3",
    "tiny-invariant": "^1.2.0",
    "typeface-yantramanav": "^1.1.13",
    "typescript": "^4.5.4",
    "web-vitals": "^2.1.2"
  },
  "lint-staged": {
    "*.+(json|yml|yaml|css|less|scss|html)": [
      "yarn format"
    ],
    "*.+(js|jsx|ts|tsx)": [
      "yarn lint:fix",
      "yarn format"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn check-types && yarn lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.1",
    "@testing-library/user-event": "^13.5.0",
    "@types/d3": "^7.1.0",
    "@types/d3-ease": "^3.0.0",
    "@types/jest": "^27.0.3",
    "@types/matter-js": "^0.18.2",
    "@types/node": "^17.0.5",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.11",
    "@types/react-map-gl": "^6.1.1",
    "@types/styled-components": "^5.1.19",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.4",
    "pinst": "^2.1.6",
    "prettier": "2.5.1"
  },
  "resolutions": {
    "babel-loader": "8.1.0",
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@latest"
  }
}
